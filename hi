AutoHoops.Name = "AutoHoops"
AutoHoops.Parent = AutoFarmFrame
AutoHoops.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
AutoHoops.BorderColor3 = Color3.fromRGB(0, 0, 0)
AutoHoops.BorderSizePixel = 0
AutoHoops.Position = UDim2.new(0.0303030312, 0, 0, 0)
AutoHoops.Size = UDim2.new(0, 277, 0, 42)
AutoHoops.Font = Enum.Font.Fantasy
AutoHoops.Text = "Auto Hoops"
AutoHoops.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoHoops.TextSize = 14.000
AutoHoops.MouseButton1Down:connect(function()
	wait(1)
	_G.Farm3 = true

	while _G.Farm3 == true do wait()
		for i,v in pairs(game.Workspace.Hoops:GetChildren()) do
			v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		end
	end
end)

UICorner_4.Parent = AutoHoops

AutoGems.Name = "AutoGems"
AutoGems.Parent = AutoFarmFrame
AutoGems.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
AutoGems.BorderColor3 = Color3.fromRGB(0, 0, 0)
AutoGems.BorderSizePixel = 0
AutoGems.Position = UDim2.new(0.0303030312, 0, 0.266666681, 0)
AutoGems.Size = UDim2.new(0, 277, 0, 38)
AutoGems.Font = Enum.Font.Fantasy
AutoGems.Text = "Auto Gems"
AutoGems.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoGems.TextSize = 14.000
AutoGems.MouseButton1Down:connect(function()
	wait(1)
	_G.Farm2 = true

	while _G.Farm2 == true do wait()
		for i,v in pairs(game.Workspace.orbFolder.City:GetChildren()) do
			if v.Name == "Gem" then
				v.outerGem.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			end
		end
	end
end)

UICorner_5.Parent = AutoGems

AutoRebirths.Name = "AutoRebirths"
AutoRebirths.Parent = AutoFarmFrame
AutoRebirths.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
AutoRebirths.BorderColor3 = Color3.fromRGB(0, 0, 0)
AutoRebirths.BorderSizePixel = 0
AutoRebirths.Position = UDim2.new(0.0303030312, 0, 0.50555557, 0)
AutoRebirths.Size = UDim2.new(0, 277, 0, 36)
AutoRebirths.Font = Enum.Font.Fantasy
AutoRebirths.Text = "Auto Rebirths"
AutoRebirths.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoRebirths.TextSize = 14.000
AutoRebirths.MouseButton1Down:connect(function()
	wait(1)
	_G.Rebirth = true

	while _G.Rebirth == true do wait(5)
		local tbl_main = 
			{
				"rebirthRequest"
			}
		game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer(unpack(tbl_main))
	end
end)

UICorner_6.Parent = AutoRebirths

AutoOrbs.Name = "AutoOrbs"
AutoOrbs.Parent = AutoFarmFrame
AutoOrbs.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
AutoOrbs.BorderColor3 = Color3.fromRGB(0, 0, 0)
AutoOrbs.BorderSizePixel = 0
AutoOrbs.Position = UDim2.new(0.0303030312, 0, 0.73888886, 0)
AutoOrbs.Size = UDim2.new(0, 277, 0, 36)
AutoOrbs.Font = Enum.Font.Fantasy
AutoOrbs.Text = "Auto Orbs"
AutoOrbs.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoOrbs.TextSize = 14.000
AutoOrbs.MouseButton1Down:connect(function()
	wait(1)
	_G.Farm1 = true

	while _G.Farm1 == true do wait()
		for i,v in pairs(game.Workspace.orbFolder.City:GetChildren()) do
			if v.Name ~= "Gem" then
				v.outerOrb.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			end
		end
	end
end)

